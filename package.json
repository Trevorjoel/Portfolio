{
  "name": "portfolio-backend",
  "Author": "Trevor Garrity",
  "version": "1.0.0",
  "description": "Portfolio",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "server.js",
    "server": "nodemon server.js --exec babel-node --",
    "no-nodemon-server": "server.js --exec babel-node --",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "front": "concurrently \"npm run no-nodemon-server\" \"npm run client\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "heroku-postbuild": "cd client && npm install && npm run build"
  },
  "proxy": "http://localhost:5000",
  "author": "Trevor Garrity",
  "license": "MIT",
  "dependencies": {
    "availity-reactstrap-validation": "^2.6.1",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.2",
    "express": "^4.17.1",
    "express-validator": "^6.3.1",
    "lodash": "^4.17.15",
    "mysql": "^2.17.1",
    "nodemailer": "^6.4.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "nodemon": "^1.19.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Trevorjoel/portfolio.git"
  },
  "bugs": {
    "url": "https://github.com/Trevorjoel/portfolio/issues"
  },
  "homepage": "https://github.com/Trevorjoel/portfolio#readme"
}
